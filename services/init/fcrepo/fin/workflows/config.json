{
  "defaults" : {
    "tmpGcsBucket" : "dams-workflow-tmp-space",
    "gcsBucket" : "dams-client-media-{{GCS_BUCKET_ENV}}",
    "notifyOnSuccess" : "/svc:reindex?no-crawl=true"
  },

  "definitions" : {

    "pdf-image-products" : {
      "type" : "gc-workflow",
      "data" : {
        "gcsSubpath" : "images",
        "jobContainer" : "{{IMAGE_UTILS_SERVICE_CONTAINER}}",
        "baseUrl" : "{{IMAGE_UTILS_SERVICE_URL}}"
      }
    },

    "video-to-stream" : {
      "type" : "gc-workflow",
      "data" : {
        "gcsSubpath" : "streaming",
        "jobContainer" : "{{IMAGE_UTILS_SERVICE_CONTAINER}}"
      }
    },

    "image-products" : {
      "type" : "gc-workflow",
      "data" : {
        "gcsSubpath" : "images",
        "jobContainer" : "{{IMAGE_UTILS_SERVICE_CONTAINER}}"
      }
    }

  }
}